class Solution(object):
    def romanToInt(self, s):
        d = {'I': 1, 'V': 5, 'X': 10, 'L': 50, 'C': 100, 'D': 500, 'M': 1000}
        x = 0
        for i in range(0,len(s)-1):
            if d[s[i]]< d[s[i+1]]:
                x -=d[s[i]]
            else:
                x += d[s[i]]
        x += d[s[-1]]
        return(x)
